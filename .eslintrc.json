{
  "rules": {
    "indent": [
      2,
      "white-space"
    ],
    "quotes": [
      2,
      "double"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended",
}